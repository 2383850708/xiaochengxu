<template name="category">
  <swiper class='category' indicator-dots="true"  indicator-active-color="#DE2A1F" >
    <block wx:for="{{categoryList}}"  wx:for-index="idx"  wx:for-item="over">
      <swiper-item>              
        <view class="category-top "  style="display: flex;flex-direction: row; flex-wrap: wrap;">
          <block wx:for="{{over}}"   wx:for-item="item">
            <view  style="margin-bottom:20rpx;width:20%" >
              <view style="width:90rpx;height:90rpx;margin:0px auto;">
                <image src="{{item.imgpath}}"  style="width:100%;height:100%" />
              </view>
              <view  class="category-font">{{item.title}}</view>
            </view>
          </block>
        </view>
      </swiper-item>
    </block>
  </swiper>
  

